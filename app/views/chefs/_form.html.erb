<%= render 'shared/errors', obj: @chef %>

<div class="row">
  <div class="well col-md-8 col-md-offset-2">
    <%= form_for(@chef) do |f| %>
        <%= f.label :chefname %>
        <%= f.text_field :chefname %><br>

        <%= f.label :email %>
        <%= f.email_field :email %><br>

        <%= f.label :password %>
        <%= f.password_field :password %><br>


        <%= f.submit(@chef.new_record? ? 'Register': 'Save Profile Changes', class: 'btn btn-success') %>
    <% end %>


  </div>
</div>